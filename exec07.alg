algoritmo "semnome"
var
       V1,T,T2: Real
inicio
      Escreval("_________BORA_________")
      Escreval("Digite um valor real")
      Leia(V1)
      T <- (V1*2)
      T2 <- (V1/3)
      Escreval("O dobro desse valor será: ",T)
      Escreval("A terça parte desse valor será: ",T2)
      Escreval("__________FIM__________")




fimalgoritmo
